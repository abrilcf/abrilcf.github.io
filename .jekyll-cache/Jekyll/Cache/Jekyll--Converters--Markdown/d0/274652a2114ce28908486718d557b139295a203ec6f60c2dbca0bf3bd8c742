  <span class="na">booktitle</span> <span class="p">=</span> <span class="s">{2022 44th Annual International Conference of the IEEE Engineering in Medicine &amp; Biology Society (EMBC)}</span><span class="p">,</span>
  <span class="na">title</span> <span class="p">=</span> <span class="s">{MedNeRF: Medical Neural Radiance Fields for Reconstructing 3D-aware CT-Projections from a Single X-ray}</span><span class="p">,</span>
  <span class="na">year</span> <span class="p">=</span> <span class="s">{2022}</span><span class="p">,</span>
